---
import data from "../data/site.json";

import Button from "../components/Button.astro";
const title = "Game on, Level up!";
---

<div class="bg-gradient-to-b from-white dark:from-slate-600">
  <div class="pt-16 pb-8">
    <div class="text-center justify-self-center items-center p-2">
      <h1
        class="text-4xl font-bold tracking-tight text-gray-900 dark:text-white sm:text-6xl"
      >
        {title}
      </h1>

      <div class="mt-10 flex items-center justify-center gap-x-2">
        <Button text="Personal Projects ->" href="/projects" />
      </div>
    </div>
  </div>
</div>
